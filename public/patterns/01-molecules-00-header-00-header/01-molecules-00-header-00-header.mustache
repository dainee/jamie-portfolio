{{#header}}
<section class="header">
  <nav class="navbar navbar-expand-md">
    <div class="container">
      <a class="navbar-brand" href="{{url}}"><img src="../../visuals/css-assets/jamie-logo.svg"></a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#{{id}}" aria-controls="{{id}}" aria-expanded="false" aria-label="Toggle navigation">
			</button>
      <div class="collapse navbar-collapse" id="{{id}}">
        <ul class="navbar-nav">
          {{#navItem}}
          <li class="nav-item{{#active}} active{{/active}}">
            <a class="nav-link" href="{{url}}">{{text}}</a>
          </li>
          {{/navItem}}
        </ul>
      </div>
    </div>
  </nav>
</section>
{{/header}}