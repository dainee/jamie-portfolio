{
    "env": {
        "browser": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [1, "tab", {"SwitchCase": 1}],
        "no-undefined": [0],
        "linebreak-style": [1, "unix"],
        "quotes": [1, "single"],
        "semi": [1, "always"],
        "space-in-parens": [1, "always"],
        "no-console": [1],
        "no-alert": [1],
        "camelcase": [1]
    }
}